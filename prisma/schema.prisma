// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
}

// schema starts here
enum UserRole {
  FEMBOY
  RENTER
}

model User {
  id        String   @id @default(cuid())
  email     String   @unique
  username  String
  role      UserRole
  createdAt DateTime @default(now())
}
